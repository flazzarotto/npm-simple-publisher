{
	"name": "@kebab-case/npm-simple-publisher",
	"version": "2.0.4",
	"description": "nodejs command-line script allowing to easily compile and publish both node and es6 compliant code packages to npm.",
	"author": "kebab-case",
	"homepage": "https://github.com/flazzarotto/npm-simple-publisher",
	"repository": {
		"type": "git",
		"url": "https://github.com/flazzarotto/npm-simple-publisher.git"
	},
	"bugs": {
		"url": "https://github.com/flazzarotto/npm-simple-publisher/issues"
	},
	"main": "./dist/index.js",
	"license": "MIT",
	"keywords": [
		"command-line",
		"publisher",
		"easy",
		"simple",
		"npm",
		"babel",
		"compile",
		"es6",
		"nodejs",
		"yarn"
	],
	"private": false,
	"scripts": {
		"lint": "eslint src",
		"build": "yarn lint && rm -rf dist && babel src -d dist && chmod +x ./dist/*.js"
	},
	"bin": {
		"kc-nsp": "./dist/bin.js"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.8",
		"@babel/core": "^7.12.9",
		"@babel/preset-env": "^7.12.7",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.15.0"
	},
	"dependencies": {
		"@kebab-case/node-command-manager": "^1.3.10",
		"argv": "^0.0.2",
		"axios": "^0.21.0",
		"core-js": "^3.8.0",
		"fs": "^0.0.1-security",
		"prompt-async": "^0.9.9"
	},
	"module": "./src/index.js",
	"eslintConfig": {
		"root": true,
		"env": {
			"es6": true,
			"node": true
		},
		"extends": [
			"eslint:recommended"
		],
		"parser": "babel-eslint",
		"parserOptions": {},
		"rules": {}
	}
}